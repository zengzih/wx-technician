.wrapper {
    height: 100%;
    background: #f8f8f8;
}

.wrapper .head {
    height: 416rpx;
    position: relative;
    margin-bottom: 20rpx;
}

.wrapper .head .bgm {
    position: absolute;
    height: 358rpx;
    width: 100%;
    background: #0077aa;
    top: 0;
    left: 0;
}

.wrapper .head .con {
    height: 330rpx;
    width: 710rpx;
    position: relative;
    top: 85rpx;
    margin: 0 auto;
    background: #fff;
    border-radius: 4px;
}

.wrapper .head .con .icon {
    position: absolute;
    top: -48rpx;
    width: 156rpx;
    height: 156rpx;
    border-radius: 4px;
    left: 26rpx;
    overflow: hidden;
    background: #fff;
}

.wrapper .head .con .icon image {
    width: 100%;
    height: 100%;
}


.wrapper .head .con .icon-info {
    height: 165rpx;
    position: relative;
}

.wrapper .head .con .info {
    position: absolute;
    left: 210rpx;
    top: 0;
    height: 120rpx;
    padding-top: 20rpx;
    box-sizing: border-box;
}

.wrapper .head .con .info .name {
    font-size: 32rpx;
    font-weight: bold;
    margin-bottom: 20rpx;
}

.wrapper .head .con .info .phone {
    font-size: 22rpx;
    color: #6b6b6b;
}

.wrapper .head .con-list .list {
    display: flex;
}

.wrapper .head .con-list .list>view {
    flex: 2;
    text-align: center;
}

.wrapper .head .con-list .list>view .num {
    font-size: 28rpx;
    margin-bottom: 18rpx;
}

.wrapper .head .con-list .list>view .label {
    font-size: 22rpx;
    color: #999999;
}

/*   订单    */
.order-content {
    padding: 0 20rpx;
    box-sizing: border-box;
    margin-bottom: 20rpx;
}

.order-content .content {
    background: #fff;
    border-radius: 4px;
    display: flex;
    padding: 20rpx 0;
}

.order-content .group {
    flex: 2;
}

.order-content .group {
    text-align: center;
}

.order-content .group .icon {
    width: 60rpx;
    height: 50rpx;
    overflow: hidden;
    margin-bottom: 18rpx;
    display: inline-block;
}


.order-content .group .icon i {
    font-size: 50rpx;
}

.order-content .group .name {
    font-size: 26rpx;
    color: #969696;
}



/*  end */

.content {
    padding: 0 20rpx;
    border-radius: 4rpx;
    margin-bottom: 20rpx;
}

.content .con {
    margin-bottom: 30rpx;
    background: #fff;
}

.content .con .list-group {
    padding: 0 28rpx;
    height: 100rpx;
    text-align: center;
    border: 1px solid 1px solid #f7f7f7;
    box-sizing: border-box;
    line-height: 100rpx;
    font-size: 30rpx;
}


/*  功能列表 */
.list-content {
    padding: 0 20rpx;
    border-radius: 4px;
    box-sizing: border-box;
    margin-bottom: 20rpx;
}

.list-content .list-group {
    background: #fff;
    padding: 0 28rpx;
    height: 100rpx;
    text-align: left;
    box-sizing: border-box;
    line-height: 100rpx;
    border-bottom: 1px solid #f7f7f7;
    font-size: 30rpx;
}

.list-content .list-group i {
    float: right;
    font-size: 24rpx;
    color: #d3d3d3;
}










/* 选择项目项目详情 */

.project_detail {
    height: 100%;
    width: 100%;
    position: absolute;
    top: 0;
    left: 0;
    overflow: hidden;
}

.project_detail .mask {
    height: 100%;
    width: 100%;
    position: absolute;
    top: 0;
    left: 0;
    opacity: 0.5;
    background: #000;
    z-index: 3;
}
/*  优惠券 */
.coupons {
    height: 800rpx;
    width: 100%;
    background: #fff;
    padding: 20rpx  40rpx;
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 5;
    overflow: hidden;
    box-sizing: border-box;
}

.coupons .head {
    height: 50rpx;
    line-height: 50rpx;
    text-align: center;
    font-size: 32rpx;
    position: relative;
}

.coupons .head .close {
    position: absolute;
    top: 0;
    right: 0;
    width: 40rpx;
    height: 40rpx;
    line-height: 40rpx;
    text-align: center;
}

.coupons .head .close i {
    font-size: 24rpx;
}

.coupons .content {
    margin-top: 30rpx;
}

.coupons .content .list {
    height: 150rpx;
    background: #fff5f6;
    border-radius: 4px;
    overflow: hidden;
}

.coupons .content .list>view {
    float: left;
    padding: 20rpx 0;
    box-sizing: border-box;
}

.coupons .content .list .price {
    width: 150rpx;
    height: 100%;
    text-align: center;
    font-size: 22rpx;
    color: #f73c45;
    line-height: 150rpx;
    border-right: 1px dotted #ccc;
}

.coupons .content .list .price .num {
    font-size: 40rpx;
    margin-right: 0;
    color: #f73c45;
}

.coupons .content .list .detail {
    padding: 0 40rpx;
    box-sizing: border-box;
}

.coupons .content .list .detail>view {
    height: 50rpx;
    line-height: 50rpx;
}

.coupons .content .list .detail .name{
    font-size: 30rpx;
}

.coupons .content .list .detail .tip,
.coupons .content .list .detail .more
{
    font-size: 22rpx;
    color: #a49e9e;
}

.coupons .content .list .operation {
    width: 65rpx;
    padding: 0 18rpx;
    height: 150rpx;
    line-height: 30rpx;
    box-sizing: border-box;
    background: #e16734;
    color: #fff;
    font-size: 24rpx;
    float: right;
    position: relative;
}

.coupons .content .list .operation text {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    color: #fff;
}







