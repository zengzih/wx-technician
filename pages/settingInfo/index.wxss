/* pages/settingInfo/index.wxss */
.wrapper {
    background: #fff;
}
.content {
    border-top: 1px solid #ededed;
}

.content .list {
    height: 100rpx;
    padding: 0 33rpx;
    line-height: 100rpx;
    font-size: 28rpx;
    position: relative;
    border-bottom: 1px solid #ededed;
}

.content .list i {
    position: absolute;
    right: 35rpx;
    font-size: 20rpx;
    top: 50%;
    transform: translateY(-50%);
    color: #ccc;
}
