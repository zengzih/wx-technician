.wrapper .content .body {
}

.wrapper .content .body .service-photo {
    height: 270rpx;
    border-top-left-radius: 25rpx;
    border-top-right-radius: 25rpx;
    background: #fff;
    padding: 25rpx;
    box-sizing: border-box;
    border-bottom: 1px solid #ededed;
    overflow: auto;
}

.wrapper .content .body .service-photo .photo-wrapper {
    height: 100%;
}

.wrapper .content .body .service-photo .photo-wrapper .img-box {
    width: 220rpx;
    height: 220rpx;
    padding: 10rpx;
    box-sizing: border-box;
    border-radius: 4px;
    overflow: hidden;
    float: left;
}
