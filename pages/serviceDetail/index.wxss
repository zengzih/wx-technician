.wrapper {
    height: 100%;
}

.wrapper .content {
    height: calc(100% - 150rpx);
}

.wrapper .content .head {
    height: 240rpx;
    position: relative;
    padding: 20rpx;
    box-sizing: border-box;
    background-repeat: no-repeat;
    background-attachment: fixed;
    overflow: hidden;
}

.wrapper .content .head .bg-img {
    position: absolute;
    top: 0;
    left: 0;
    height: 100%;
    width: 100%;
    z-index: -2;
}

.wrapper .content .head .bg-img image{
    height: 100%;
    width: 100%;
}

.wrapper .content .head>.bg {
    height: 100%;
    width: 100%;
    background: inherit;
    -webkit-filter: blur(5px);
    -moz-filter: blur(5px);
    -ms-filter: blur(5px);
    -o-filter: blur(5px);
    filter: blur(8px);
    filter: progid:DXImageTransform.Microsoft.Blur(PixelRadius=4, MakeShadow=false);
    position: absolute;
    top: 0;
    left: 0;
}

.wrapper .content .head .icon {
    height: 140rpx;
    width: 140rpx;
    border-radius: 50%;
    position: relative;
    overflow: hidden;
    float: left;
    position: relative;
    z-index: 2;
}

.wrapper .content .head .icon text {
    position: absolute;
    background: #9d53ff;
    color: #fff;
    width: 100%;
    height: 70rpx;
    bottom: -35rpx;
    font-size: 18rpx;
    text-align: center;
    left: 0;
}

.wrapper .content .head .icon image {
    height: 100%;
    width: 100%;
}


.wrapper .content .head .detail {
    float: left;
    position: relative;
    z-index: 2;
    margin-left: 20rpx;
}

.wrapper .content .head .detail .name {
    font-size: 26rpx;
    color: #fff;
    margin-top: 40rpx;
}

.wrapper .content .head .detail .name .tip {
    margin-left: 80rpx;
    font-size: 20rpx;
    color: #fff;
}


.wrapper .content .head .certification {
    height: 40rpx;
}

.wrapper .content .head .certification .flex {
    line-height: 40rpx;
    margin-right: 35rpx;
    float: left;
}

.wrapper .content .head .certification .flex .van-icon {
    color: red;
    position: relative;
    top: 9rpx;
}


.wrapper .content .head .certification .flex text {
    font-size: 18rpx;
    color: #fff;
    margin-left: 8rpx;
}

.wrapper .content .head .order-distance {
    height: 80rpx;
    text-align: center;
    width: 100%;
    border-top-left-radius: 25rpx;
    border-top-right-radius: 25rpx;
    position: absolute;
    left: 0;
    bottom: -10rpx;
    display: flex;
    overflow: hidden;

}

.wrapper .content .head .order-distance .bg {
    height: 100%;
    width: 100%;
    position: absolute;
    left: 0;
    top: 0;
    background: #000;
    opacity: 0.3;
}

.wrapper .content .head .order-distance .flex {
    flex: 2;
}

.wrapper .content .head .order-distance .flex .label,
.wrapper .content .head .order-distance .flex .num
{
    color: #fff;
    height: 35rpx;
    line-height: 35rpx;
    font-size: 22rpx;
    position: relative;
    z-index: 1;
}
.wrapper .content .head .order-distance .flex .num {
    font-size: 20rpx;
}


