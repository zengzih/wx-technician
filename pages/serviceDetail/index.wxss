/* 声明 */
.statement {
    height: 70rpx;
    padding: 12rpx;
    box-sizing: border-box;
    display: flex;
    background: #fff;
    margin-top: 10rpx;
}

.statement .list {
    flex: 2;
    height: 46rpx;
}

.statement .list .inner {
    width: 95%;
    height: 46rpx;
    line-height: 46rpx;
    border: 1px solid #f6f6f6;
    border-radius: 4rpx;
    text-align: center;
    font-size: 18rpx;
    color: #c93849;
    position: relative;
}

.statement .list .inner i {
    position: absolute;
    top: 50%;
    left: 5rpx;
    transform: translateY(-50%);
}

.statement .list .inner text {
    position: absolute;
    top: 50%;
    left: 40rpx;
    transform: translateY(-50%);
}

/*  视频  */
.video-wrapper {
    margin-top: 20rpx;
    background: #fff;
    height: 350rpx;
    padding: 20rpx;
    width: 100%;
    box-sizing: border-box;
    position: relative;
}

.video-wrapper video {
    width: 100%;
    height: 100%;
}

.video-wrapper .cover {
    position: absolute;
    height: 100%;
    width: 100%;
    top: 0;
    left: 0;
}

.video-wrapper .cover image {
    width: 100%;
    height: 100%;
}

.video-wrapper .cover .seconds {
    position: absolute;
    bottom: 20rpx;
    right: 20rpx;
    font-size: 24rpx;
    color: #fff;
}

/*  商品 */

/* 列表项数据 */
.product-list {
    background: #fff;
    margin-top: 20rpx;
}

.list-view .list {
    overflow: hidden;
    padding: 30rpx;
    box-sizing: border-box;
    border-bottom: 1px solid #ccc;
}

.list-view .list .head-img {
    width: 163rpx;
    height: 163rpx;
    border: 1px solid transparent;
    float: left;
    overflow: hidden;
    border-radius: 4px;
}

.list-view .list .head-img image {
    /* width: 100%;
    height: 100%; */
}

.list-view .list .detail {
    float: left;
    text-align: left;
    margin-left: 30rpx;
    width: calc(100% - 200rpx);
}

.list-view .list .detail .title {
    font-size: 30rpx;
    margin-bottom: 10rpx;
}

.list-view .list .detail .introduce {
    font-size: 25rpx;
    color: #838383;
    margin-bottom: 10rpx;
}

.list-view .list .detail .list-desc {
    font-size: 20rpx;
    color: #838383;
}

.list-view .list .detail .price {
    font-size: 28rpx;
    color: #dd3b48;
    /* margin-bottom: 10rpx; */
}

.list-view .list .detail .price text:last-child {
    margin-left: 10rpx;
    color: #909399;
    position: relative
}

.list-view .list .detail .price text:last-child:after {
    content: '';
    position: absolute;
    width: 100%;
    height: 1px;
    background: #909399;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
}

.list-view .list .detail  .more {
    overflow: hidden;
    width: 100%;
    line-height: 24rpx;
    color: #838383;
}

.list-view .list .detail .more text {
    font-size: 20rpx;
    margin-top: 6rpx;
}

.list-view .list .detail .more> text {
    float:left;
}

.list-view .list .detail .more .r-view {
    float:right;
    margin-top: -3rpx;
}

.list-view .list .detail .more .r-view text {
    padding: 0 10rpx;
}














